<div id="eduWrap">
<div id="oscView">
    <main>
        <h1>Oscillator</h1>
        <p>the core of any synthesizer</p>
        <div id="oscDetail"></div>
    </main>
    <aside>
        <div wave="sine">
        <button id="startSine"><div></div></button>
        <img src="../images/sine.png">
        </div>
        <div wave="triangle">
        <button id="startTriangle"><div></div></button>
        <img src="../images/triangle.png">
        </div>
        <div wave="square">
        <button id="startSquare"><div></div></button>
        <img src="../images/square.png">
        </div>
        <div wave="sawtooth">
        <button id="startSawtooth"><div></div></button>
        <img src="../images/sawtooth.png">
        </div>
    </aside>
    <div class="continueSection"><span id="pickOsc">continue with selected soundwave</span></div>
</div>
    <span class="closeChip">&times;</span>
</div>